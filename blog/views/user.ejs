<%- include header %>
<% posts.forEach(function (post, index) { %>
  <p><h2><a href="#"><%= post.postTitle %></a></h2></p>
  <p class="info">
    作者：<a href="#"><%= post.postName %></a> | 
    日期：<%= post.createDate.std %>
  </p>
  <p><%- post.postBody %></p>
  <p class="info">阅读：<%= post.pv %></p>
  <hr />
<% }) %>
<%- include paging %>
<%- include footer %>