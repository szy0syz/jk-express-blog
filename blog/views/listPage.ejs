<%- include header %>
<% models.forEach(function (model, index) { %>
  <p><h2><a href="#"><%= model.postTitle %></a></h2></p>
  <p class="info">
    作者：<a href="#"><%= model.postName %></a> | 
    日期：<%= model.createDate.std %>
  </p>
  <p><%- model.postBody %></p>
  <p class="info">阅读：<%= model.pv %> | 评论：<%= model.pv %> | 标签：<%- include printPostTags %>
  </p>
  <hr />
<% }) %>
<%- include paging %>
<%- include footer %>